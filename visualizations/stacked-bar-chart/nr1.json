{
  "schemaType": "VISUALIZATION",
  "id": "stacked-bar-chart",
  "displayName": "Stacked bar chart",
  "description": "",
  "configuration": [
    {
      "name": "nrqlQueries",
      "title": "NRQL Queries",
      "type": "collection",
      "items": [
        {
          "name": "accountId",
          "title": "Account ID",
          "description": "Account ID to be associated with the query",
          "type": "account-id"
        },
        {
          "name": "query",
          "title": "Query",
          "description": "NRQL query for visualization",
          "type": "nrql",
          "defaultValue": "FROM PageView SELECT count(*) FACET countryCode, userAgentName"
        }
      ],
      "defaultValue": [
        {
          "query": "FROM Transaction SELECT average(duration) FACET environment, appName"
        }
      ]
    },
    {
      "name": "yAxis",
      "title": "Y-axis configuration",
      "type": "namespace",
      "items": [
        {
          "name": "label",
          "title": "Label",
          "description": "Custom label for y-axis",
          "type": "string",
          "defaultValue": "Avg Duration (seconds)"
        }
      ]
    },
    {
      "name": "other",
      "title": "Other groups",
      "type": "namespace",
      "items": [
        {
          "name": "visible",
          "title": "Visible",
          "description": "Manages the visibility of \"Other\" groups of attributes",
          "type": "boolean",
          "defaultValue": true
        }
      ]
    }
  ]
}
